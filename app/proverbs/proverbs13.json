        "verse": "Proverbs 13:25",
        "verseID": "Proverbs_13_",
        "imageUrl": "img/img-category/",
        "category": "NA",
        "categoryID": "NA",
        "text": "25 The righteous eats to the satisfying of his soul, But the stomach of the wicked shall be in want."
    }
